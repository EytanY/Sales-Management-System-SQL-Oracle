<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.AddNewItemDeliveryController"
      spacing="5.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Text fx:id="titleTxt"/>
    <Text text="Choose Item ID:"/>
    <ChoiceBox fx:id="itemsIDChoice"/>
    <Text text="Enter Amount:"/>
    <TextField fx:id="amountTF"/>
    <Label fx:id="resultLabel"/>
    <Button text="Add" onAction="#onAddItemToDeliveryButtonClick"/>
    <Button text="Menu" fx:id="menuButton" onAction="#onReturnToMenuButtonClick"/>

</VBox>
